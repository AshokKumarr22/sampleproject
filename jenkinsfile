pipeline{
    agent any

    tools {
         maven 'maven-mvn'
         jdk 'jdk-21'
    }

    stages{
        stage('GIT CHECKOUT'){
            steps{
               git credentialsId: 'AshokKumarr22-git-access', url: 'https://github.com/AshokKumarr22/sampleproject.git'
            }
        }
        stage('BUILD'){
            steps{
               sh 'mvn clean install'
               sh 'ls -al'
               sh 'ls -al/target'
               sh 'pwd'
            }
        }
    }
}
